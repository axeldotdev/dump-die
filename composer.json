{
    "name": "axeldotdev/dump-die",
    "description": "This package give you the dd() helper from Laravel.",
    "keywords": [
        "axeldotdev",
        "laravel",
        "dump",
        "dd"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/axeldotdev/dump-die",
    "readme": "https://github.com/axeldotdev/dump-die/blob/main/README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Charpentier",
            "email": "axel.charpentier@hey.com",
            "homepage": "https://github.com/axeldotdev",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/var-dumper": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "vimeo/psalm": "^4.8"
    },
    "support": {
        "email": "axel.charpentier@hey.com",
        "source": "https://github.com/axeldotdev/dump-die",
        "issues": "https://github.com/axeldotdev/dump-die/issues",
        "docs": "https://github.com/axeldotdev/dump-die/wiki",
        "chat": "https://github.com/axeldotdev/dump-die/discussions"
    },
    "autoload": {
        "files": ["src/helpers.php"]
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
